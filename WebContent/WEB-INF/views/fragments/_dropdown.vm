<li class="dropdown">
	  <a href="#" class="dropdown-toggle" data-toggle="dropdown" 
	  role="button" aria-haspopup="true" aria-expanded="false">$myPanel.getLabel() <span class="caret"></span></a>
	  <ul class="dropdown-menu">
	  	$layoutService.in($myPanel.getSection())
			#foreach($myLink in $layoutService.getLinks())
				<li><a href="$myLink.getHref()">$myLink.getLabel()</a></li>
			#end
		$layoutService.up($myPanel.getSection())
	  </ul>
</li>